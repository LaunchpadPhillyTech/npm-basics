{
    "name": "semver-management-demo",
    "version": "3.2.1",
    "description": "Demonstrates advanced semantic versioning techniques",
    "main": "dist/index.js",
    "scripts": {
        "audit": "npm audit && npm outdated",
        "update": "npm update",
        "update:major": "ncu -u && npm install",
        "check-versions": "node scripts/check-versions.js",
        "test:versions": "npm run test:node14 && npm run test:node16 && npm run test:node18",
        "test:node14": "docker run --rm -v $(pwd):/app node:14-alpine npm test",
        "test:node16": "docker run --rm -v $(pwd):/app node:16-alpine npm test",
        "test:node18": "docker run --rm -v $(pwd):/app node:18-alpine npm test"
    },
    "dependencies": {
        "lodash": "^4.17.21",
        "axios": "~1.2.0",
        "express": ">=4.17.1 <4.19.0",
        "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
        "moment": "2.29.4",
        "chalk": "~4.1.2",
        "yargs": "^17.6.0",
        "joi": ">=17.6.0 <=17.7.0"
    },
    "devDependencies": {
        "jest": "^29.3.0",
        "eslint": "~8.28.0",
        "typescript": "^4.9.0",
        "webpack": "5.70.* || ^5.75.0",
        "babel-core": "^7.20.0",
        "prettier": "~2.8.0",
        "husky": "^8.0.0",
        "lint-staged": "^13.1.0",
        "npm-check-updates": "^16.6.0"
    },
    "peerDependencies": {
        "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": false
        },
        "react-dom": {
            "optional": false
        }
    },
    "engines": {
        "node": ">=14.0.0 <20.0.0",
        "npm": ">=6.14.0",
        "yarn": ">=1.22.0"
    },
    "overrides": {
        "semver": "^7.3.8",
        "minimatch": "^5.0.0"
    }
}